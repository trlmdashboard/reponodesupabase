{
  "version": 2,
  "builds": [
    { "src": "api/register.js", "use": "@vercel/node" },
    { "src": "*.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/register", "dest": "/api/register.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "env": {
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_KEY": "@supabase_key"
  }
}
