{
  "name": "custom-login-system",
  "version": "1.0.0",
  "description": "Custom login system with Supabase",
  "scripts": {
    "dev": "vercel dev"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0"
  },
  "devDependencies": {
    "vercel": "^28.0.0"
  },
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/.*", "dest": "/api/login.js" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ],
  "installCommand": "npm install"
}
